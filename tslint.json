{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "quotemark": [true, "single"],
      "one-line": [
        true,
        "check-open-brace",
        "check-whitespace"
      ],
      "no-console": [false],
      "object-literal-sort-keys": false,
      "ordered-imports": false,
      "max-line-length": [true, 180],
      "semicolon": true,
      "max-classes-per-file": false,
      "arrow-parens": false
    },
    "rulesDirectory": []
}
